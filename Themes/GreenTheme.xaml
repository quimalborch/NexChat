<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Green Theme Color Palette -->
    
    <!-- Accent Colors -->
    <Color x:Key="SystemAccentColor">#43A047</Color>
    <Color x:Key="SystemAccentColorLight1">#66BB6A</Color>
    <Color x:Key="SystemAccentColorLight2">#81C784</Color>
    <Color x:Key="SystemAccentColorLight3">#A5D6A7</Color>
    <Color x:Key="SystemAccentColorDark1">#388E3C</Color>
    <Color x:Key="SystemAccentColorDark2">#2E7D32</Color>
    <Color x:Key="SystemAccentColorDark3">#1B5E20</Color>

    <!-- Accent Brushes -->
    <SolidColorBrush x:Key="AccentFillColorDefaultBrush" Color="#43A047"/>
    <SolidColorBrush x:Key="AccentFillColorSecondaryBrush" Color="#43A04733"/>
    <SolidColorBrush x:Key="AccentFillColorTertiaryBrush" Color="#43A04724"/>
    <SolidColorBrush x:Key="AccentFillColorDisabledBrush" Color="#43A04759"/>
    <SolidColorBrush x:Key="AccentFillColorSelectedTextBackgroundBrush" Color="#43A047"/>

    <!-- Accent Text -->
    <SolidColorBrush x:Key="AccentTextFillColorPrimaryBrush" Color="#66BB6A"/>
    <SolidColorBrush x:Key="AccentTextFillColorSecondaryBrush" Color="#43A047"/>
    <SolidColorBrush x:Key="AccentTextFillColorTertiaryBrush" Color="#388E3C"/>
    <SolidColorBrush x:Key="AccentTextFillColorDisabledBrush" Color="#43A04759"/>

    <!-- System Fills -->
    <SolidColorBrush x:Key="SystemFillColorAttentionBrush" Color="#43A04714"/>
    <SolidColorBrush x:Key="SystemFillColorSuccessBrush" Color="#43A04714"/>
    <SolidColorBrush x:Key="SystemFillColorCautionBrush" Color="#F59E0B14"/>
    <SolidColorBrush x:Key="SystemFillColorCriticalBrush" Color="#DC262614"/>
    <SolidColorBrush x:Key="SystemFillColorNeutralBrush" Color="#FFFFFF0F"/>

    <!-- Additional Green Theme Variations -->
    <SolidColorBrush x:Key="SubtleFillColorSecondaryBrush" Color="#43A0471A"/>
    <SolidColorBrush x:Key="ControlStrokeColorOnAccentDefaultBrush" Color="#FFFFFF1F"/>
    <SolidColorBrush x:Key="ControlStrokeColorOnAccentSecondaryBrush" Color="#FFFFFF29"/>
    <SolidColorBrush x:Key="ControlStrokeColorOnAccentTertiaryBrush" Color="#FFFFFF0F"/>
    <SolidColorBrush x:Key="ControlStrokeColorOnAccentDisabledBrush" Color="#FFFFFF14"/>

</ResourceDictionary>
